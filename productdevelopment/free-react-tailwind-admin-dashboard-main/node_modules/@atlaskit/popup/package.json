{
	"name": "@atlaskit/popup",
	"version": "4.5.0",
	"description": "A popup displays brief content in an overlay.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Popup",
			"category": "Overlays and layering"
		}
	},
	"dependencies": {
		"@atlaskit/css": "^0.15.0",
		"@atlaskit/ds-lib": "^5.1.0",
		"@atlaskit/layering": "^3.2.0",
		"@atlaskit/platform-feature-flags": "^1.1.0",
		"@atlaskit/popper": "^7.1.0",
		"@atlaskit/portal": "^5.1.0",
		"@atlaskit/primitives": "^16.1.0",
		"@atlaskit/tokens": "^7.0.0",
		"@babel/runtime": "^7.0.0",
		"@compiled/react": "^0.18.6",
		"bind-event-listener": "^3.0.0",
		"focus-trap": "^2.4.5",
		"memoize-one": "^6.0.0",
		"tiny-invariant": "^1.2.0"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/button": "^23.5.0",
		"@atlaskit/code": "^17.3.0",
		"@atlaskit/docs": "^11.2.0",
		"@atlaskit/form": "^14.2.0",
		"@atlaskit/heading": "^5.2.0",
		"@atlaskit/icon": "^28.5.0",
		"@atlaskit/link": "^3.2.0",
		"@atlaskit/modal-dialog": "^14.6.0",
		"@atlaskit/section-message": "^8.8.0",
		"@atlaskit/ssr": "workspace:^",
		"@atlaskit/textfield": "^8.0.0",
		"@atlaskit/toggle": "^15.1.0",
		"@atlassian/feature-flags-test-utils": "^1.0.0",
		"@atlassian/ssr-tests": "^0.3.0",
		"@testing-library/react": "^13.4.0",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.4.3",
		"ast-types": "^0.13.3",
		"jscodeshift": "^17.0.0",
		"raf-stub": "^2.0.1",
		"react-dom": "^18.2.0"
	},
	"keywords": [
		"atlaskit",
		"react",
		"ui"
	],
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"ui-components": "lite-mode",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"static",
				"emotion"
			]
		}
	},
	"platform-feature-flags": {
		"platform_dst_popup-disable-focuslock": {
			"type": "boolean"
		},
		"platform-design-system-popup-ref": {
			"type": "boolean"
		},
		"platform-design-system-apply-popup-wrapper-focus": {
			"type": "boolean"
		},
		"fix-dropdown-close-outside-iframe": {
			"type": "boolean"
		},
		"popup-onclose-fix": {
			"type": "boolean"
		}
	},
	"homepage": "https://atlassian.design/components/popup/"
}
